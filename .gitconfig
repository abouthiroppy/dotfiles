[user]
	name = abouthiroppy
	email = git@about-hiroppy.com
[help]
	autocorrect = 1
[url "git@github.com:"]
	pushinsteadof = https://github.com/
[core]
  editor = vim
  autocrlf = input
[merge]
  ff = true
[http]
  sslVerify = false
[alias]
  st = status
  co = checkout
  so = remote show origin
  ft = fetch
  wc = whatchanged
  ci = commit
  br = branch
  cp = cherry-pick
  rb = rebase

[push]
  default = tracking 
